project name sesiones

type integer
type string
type bool
type time


entity participante_sesion  {
	id:integer
	sesion:sesion
	rol_participante_sesion:rol_participante_sesion
	ente:integer
}

entity sesion  {
	id                 :integer
	Descripcion        :string
	Fecha_Creacion      :time
	Fecha_Modificacion  :time
	Fecha_Inicio        :time
	Fecha_Fin           :time
	Periodo            :integer
	Recurrente         :bool
	Numero_Recurrencias :integer
	tipo_sesion         :tipo_sesion
}

entity tipo_sesion  {
	id                :integer
	Nombre            :string
	Descripcion       :string
	Codigo_Abreviacion :string
	Activo            :bool
	Numero_Orden       :integer
}

entity Relacion_sesiones  {
	id          :integer
	sesion_Padre :sesion
	sesion_Hijo  :sesion
}

entity rol_participante_sesion  {
	id                :integer
	Nombre            :string
	Descripcion       :string
	Codigo_Abreviacion :string
	Activo            :bool
	Numero_Orden       :integer
}
