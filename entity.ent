project name persona

type integer
type string
type bool
type time

entity genero  {
	id                :integer
	Nombre            :string
	Descripcion       :string
	Codigo_Abreviacion :string
	Activo            :bool
	Numero_Orden       :integer
}

entity grupo_etnico  {
	id                :integer
	Nombre            :string
	Descripcion       :string
	Codigo_Abreviacion :string
	Activo            :bool
	Numero_Orden       :integer
}

entity estado_civil  {
	id                :integer
	Nombre            :string
	Descripcion       :string
	Codigo_Abreviacion :string
	Activo            :bool
	Numero_Orden       :integer
}

entity tipo_discapacidad  {
	id                :integer
	Nombre            :string
	Descripcion       :string
	Codigo_Abreviacion :string
	Activo            :bool
	Numero_Orden       :integer
}

entity persona {
	id				:integer
	Primer_Nombre    :string    
	Segundo_Nombre   :string    
	Primer_Apellido  :string    
	Segundo_Apellido :string    
	Fecha_Nacimiento :time
	Usuario         :string   
